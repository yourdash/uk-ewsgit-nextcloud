#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/dist/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/dist/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/tsx@4.19.2/node_modules:/mnt/c/Users/ewsgi/projects/yourdash/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/tsx@4.19.2/node_modules/tsx/dist/cli.mjs" "$@"
fi
